mailboxBase(dir) ::= <<
virtual_mailbox_base = <dir>
>>

mailboxMaps(file) ::= <<
virtual_mailbox_maps = mysql:<file>
>>

aliasMaps(file) ::= <<
virtual_alias_maps = mysql:<file>
>>

mailboxDomains(file) ::= <<
virtual_mailbox_domains = mysql:<file>
>>

minimumUid(uid) ::= <<
virtual_minimum_uid = <uid>
>>

uidMaps(uid) ::= <<
virtual_uid_maps = static:<uid>
>>

gidMaps(gid) ::= <<
virtual_gid_maps = static:<gid>
>>
