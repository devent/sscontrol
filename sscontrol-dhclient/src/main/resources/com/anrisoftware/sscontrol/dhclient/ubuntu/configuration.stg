prependSearch() ::= <<
#?prepend .*;
>>

prepend(declaration) ::= <<
prepend <declaration.option> <declaration.declaration>;
>>

optionSearch() ::= <<
#?option .*;
>>

option(declaration) ::= <<
option <declaration.declaration>;
>>

sendSearch() ::= <<
#?send .*;
>>

send(declaration) ::= <<
send <declaration.option> <declaration.declaration>;
>>

requestSearch() ::= <<
#?request ([\w\d\.\-,\s]?)*;
>>

request(requests) ::= <<
request <requests; separator=", ">;
>>

