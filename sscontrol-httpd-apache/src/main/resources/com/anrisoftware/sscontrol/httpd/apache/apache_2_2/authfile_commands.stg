appendBasicPasswordFile(command, auth, file, users) ::= <<
<users:{ user | <command> -b <file> <user.name> <user.password>};separator="\n">

>>

appendDigestPasswordFile(command, auth, file, users) ::= <<
<users:{ user | (echo -n "<user.name>:<auth.locationFilename>:" && echo -n "<user.name>:<auth.locationFilename>:<user.password>" | md5sum) | cut -d ' ' -f 1 >\> <file>};separator="\n">

>>
