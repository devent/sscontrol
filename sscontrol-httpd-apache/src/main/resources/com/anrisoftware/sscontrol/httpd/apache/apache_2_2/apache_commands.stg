enableMods(command, mods) ::= <<
<mods:{ mod | <command> <mod>};separator="\n">
>>

createPasswordFile(command, file, users) ::= <<
<command> -c <file> <first(users):{ user | <user.name>}> <first(users):{ user | <user.password>}>
<rest(users):{ user | <command> <file> <user.name> <user.password>};separator="\n">
>>

appendPasswordFile(command, file, users) ::= <<
<users:{ user | <command> <file> <user.name> <user.password>};separator="\n">
>>
