reconfigure(args) ::= <<
<! - args.command - !>
<args.command> phpmyadmin

>>

importTables(args) ::= <<
<! - args.zcatCommand - !>
<! - args.mysqlCommand - !>
<! - args.admin - !>
<! - args.user - !>
<! - args.script - !>
<args.zcatCommand> <args.script> | <args.mysqlCommand> --user=<args.admin.user> --password=<args.admin.password> <args.user.database>

>>
