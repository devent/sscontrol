unix(args) ::= <<
<args.command> <unixGroups(args)> <unixAppendGroups(args)> <unixShell(args)> <args.userName>

>>

unixGroups(args) ::= <%
<if(args.groups)>-G <args.groups;separator=","><endif>
%>

unixAppendGroups(args) ::= <%
<if(args.append)>-a<endif>
%>

unixShell(args) ::= <%
<if(args.shell)>-s "<args.shell>"<endif>
%>
