cabalInstallCommand(args) ::= <<
<! - args.command - !>
<! - args.packages - !>
<! - args.extras - !>
<args.command> update && <args.command> install <args.extras;separator=" "> <args.packages;separator=" ">

>>

printDefaultConfigCommand(args) ::= <<
<! - args.command - !>
<args.command> --print-default-config

>>
