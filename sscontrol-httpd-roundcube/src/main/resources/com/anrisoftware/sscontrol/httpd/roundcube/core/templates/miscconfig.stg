
configPlugins_search() ::= <%
(?ms)^\$config\['plugins'\] = array\(.*\);
%>

configPlugins(plugins) ::= <<
$config['plugins'] = array(
<plugins:{p|'<p>'};separator=",\n">
);
>>

configDesKey_search() ::= <%
(?m)^\$config\['des_key'\]\s+=\s+.*
%>

configDesKey(key) ::= <%
$config['des_key'] = '<key>';
%>
