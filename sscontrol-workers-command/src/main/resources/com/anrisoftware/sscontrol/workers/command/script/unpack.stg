unix(type, command, output, file, override) ::= <<
#!/bin/sh
<(type)(command, output, file, override)>

>>

tgz(command, output, file, override) ::= <<
<tar(command, output, file, override)>
>>

tar(command, output, file, override) ::= <<
cd <output>
<command> <if(override)>--overwrite<endif> -xf <file>

>>
